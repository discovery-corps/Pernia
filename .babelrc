{
  "sourceMaps": "both",
  "compact": false,
  "comments": true,
  "presets": [
    "modern-node"
  ],
  "env": {
    "devel": {
      "plugins": [
        "react-hot-loader/babel"
      ],
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ]
      ]
    },
    "production": {
      "presets": [
        "react",
        "es2015"
      ]
    }
  }
}
